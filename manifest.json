{
    "name": "Linkedin Connection Request Acceptor",
    "description": "Accepts all Linkedin connection request in one go.",
    "version": "0.0.1",
    "manifest_version": 3,
    "icons": {
        "16": "/images/icons8-linkedin-16.png",
        "32": "/images/icons8-linkedin-32.png",
        "48": "/images/icons8-linkedin-48.png",
        "128": "/images/icons8-linkedin-128.png"
    },
    "action": {
        "default_title": "Click to accept all request"
    },
    "permissions": ["activeTab", "scripting" ],
    "background": {
        "service_worker": "background.js"
    }
}